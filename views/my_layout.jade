    script#selectable_options_template(type='text/template').
      <div style="display:inline-block">
        <select class="form-control cancel-padding-tb">
        <% _.each(options, function(element, index, array){%> 
          <%if ( Array.isArray(options)) {%>
            <option <%if (selected == element.toString()){%> selected <%}%>  value="<%=element%>"><%=element%></option>
          <% } else  { %>
            <option <%if (selected == element){%> selected <%}%>  value="<%=index%>"><%=element%></option>
          <%}%>
        <%}); %>
        </select> 
      </div>

    script#preview_template(type='text/template')
      .preview-container.dz-image-preview.text-center
        //-  This is used as the file preview template
        div.preview-image-container
          span.preview
            img(data-dz-thumbnail='')
        div.preview-image-cover
        div.preview-status-container.preview-info-control
          div.name(data-dz-name='')
          strong.error.text-danger(data-dz-errormessage='')
        div.preview-summary-container
          div.size.preview-info-control(data-dz-size='')
          .progress.progress-striped.active.margin-bottom-1x.margin-left-right-2x(role='progressbar', aria-valuemin='0', aria-valuemax='100', aria-valuenow='0')
            .progress-bar.progress-bar-success(style='width:0%;', data-dz-uploadprogress='')
        div.preview-buttons-container.preview-info-control
          i.glyphicon.glyphicon-upload.start(data-toggle="tooltip" data-placement="bottom" title="Upload" data-delay="0")
          i.glyphicon.glyphicon-ban-circle.cancel(data-dz-remove='' data-toggle="tooltip" data-placement="bottom" title="Cancel uploading" data-delay="0")
          i.glyphicon.glyphicon-trash.delete(data-dz-remove='' data-toggle="tooltip" data-placement="bottom" title="Remove this page" data-delay="0")
          i.glyphicon.glyphicon-picture.set-cover(data-toggle="tooltip" data-placement="bottom" title="Set as cover" data-delay="0") 
